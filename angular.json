{
  "version": 1,
  "projects": {
    "header": {
      "projectType": "library",
      "root": "libs/header",
      "sourceRoot": "libs/header/src",
      "prefix": "lpg",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/header"],
          "options": {
            "jestConfig": "libs/header/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/header/src/**/*.ts",
              "libs/header/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/header/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/header",
            "config": {
              "configFolder": "libs/header/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": []
    },
    "header-e2e": {
      "root": "apps/header-e2e",
      "sourceRoot": "apps/header-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/header-e2e/cypress.json",
            "devServerTarget": "header:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "header:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/header-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["header"]
    },
    "layout": {
      "projectType": "library",
      "root": "libs/layout",
      "sourceRoot": "libs/layout/src",
      "prefix": "lpg",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/layout"],
          "options": {
            "jestConfig": "libs/layout/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/layout/src/**/*.ts",
              "libs/layout/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "lpg-admin": {
      "projectType": "application",
      "root": "apps/lpg-admin",
      "sourceRoot": "apps/lpg-admin/src",
      "prefix": "lpg",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/lpg-admin",
            "index": "apps/lpg-admin/src/index.html",
            "main": "apps/lpg-admin/src/main.ts",
            "polyfills": "apps/lpg-admin/src/polyfills.ts",
            "tsConfig": "apps/lpg-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/lpg-admin/src/favicon.ico",
              "apps/lpg-admin/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "apps/lpg-admin/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/lpg-admin/src/environments/environment.ts",
                  "with": "apps/lpg-admin/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "lpg-admin:build:production"
            },
            "development": {
              "browserTarget": "lpg-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "lpg-admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/lpg-admin/src/**/*.ts",
              "apps/lpg-admin/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/lpg-admin"],
          "options": {
            "jestConfig": "apps/lpg-admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "lpg-admin-e2e": {
      "root": "apps/lpg-admin-e2e",
      "sourceRoot": "apps/lpg-admin-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/lpg-admin-e2e/cypress.json",
            "devServerTarget": "lpg-admin:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "lpg-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/lpg-admin-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["lpg-admin"]
    },
    "sidenav": {
      "projectType": "library",
      "root": "libs/sidenav",
      "sourceRoot": "libs/sidenav/src",
      "prefix": "lpg",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/sidenav"],
          "options": {
            "jestConfig": "libs/sidenav/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/sidenav/src/**/*.ts",
              "libs/sidenav/src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/sidenav/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/sidenav",
            "config": {
              "configFolder": "libs/sidenav/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": []
    },
    "sidenav-e2e": {
      "root": "apps/sidenav-e2e",
      "sourceRoot": "apps/sidenav-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sidenav-e2e/cypress.json",
            "devServerTarget": "sidenav:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "sidenav:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/sidenav-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["sidenav"]
    }
  }
}
